<UserControl x:Class="VoiceChangerApp.Views.SoundViews.NavigationBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VoiceChangerApp.Views.SoundViews"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <DockPanel LastChildFill="False">
            <Button Content="&lt;&lt;" DockPanel.Dock="Left"></Button>
            <Button Content="Play"></Button>
            <Button Content="Pause"></Button>
            <Button Content="Stop"></Button>
            <Button Content=">>" DockPanel.Dock="Right"></Button>            
        </DockPanel>
    </Grid>
</UserControl>
